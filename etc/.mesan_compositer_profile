if [ -f /etc/profile.d/smhi.sh -a -z "$SMHI_MODE" ]
# don't touch SMHI_MODE if it is already set.
then
. /etc/profile.d/smhi.sh
fi

case $SMHI_DIST in
linda4)

MESAN_COMPOSITE_CONFIG_DIR=/home/a000680/laptop/Satsa/Mesan/etc
export MESAN_COMPOSITE_CONFIG_DIR
PPP_CONFIG_DIR=/home/a000680/laptop/Satsa/Mesan/etc
export PPP_CONFIG_DIR

  ;;

elin4)

case $SMHI_MODE in
utv)


MESAN_COMPOSITE_CONFIG_DIR=/usr/local/etc
export MESAN_COMPOSITE_CONFIG_DIR
PPP_CONFIG_DIR=/data/proj/safutv/dev/mesan
export PPP_CONFIG_DIR

PYTHONPATH=/usr/local/lib/python2.7/site-packages:/usr/local/lib64/python2.7/site-packages
export PYTHONPATH

  ;;

###################################################################$
### Jenkins

ci)

CENTRAL=/usr/local/lib64/python2.7/site-packages:/usr/local/lib/python2.7/site-packages

PYTHONPATH=$CENTRAL:$MPOP_ETC:${PYTHONPATH}
export PYTHONPATH

MESAN_COMPOSITE_CONFIG_DIR=/usr/local/etc
export MESAN_COMPOSITE_CONFIG_DIR
PPP_CONFIG_DIR=/data/proj/safutv/mesan_etc
export PPP_CONFIG_DIR

  ;;

*)
echo "No SMHI_MODE set..."
   ;;

esac;;

elin3)


###################################################################$
### Jenkins

case $SMHI_MODE in
ci)

CENTRAL=/usr/local/lib64/python2.6/site-packages:/usr/local/lib/python2.6/site-packages

PYCOAST=/data/proj/safutv/opt/PYCOAST/

PYTHONPATH=$CENTRAL:$PYCOAST:$MPOP_ETC:${PYTHONPATH}
export PYTHONPATH

MESAN_COMPOSITE_CONFIG_DIR=/usr/local/etc
export MESAN_COMPOSITE_CONFIG_DIR
PPP_CONFIG_DIR=/data/proj/safutv/mesan_etc
export PPP_CONFIG_DIR

  ;;

*)
echo "No SMHI_MODE set..."
   ;;

esac;;


*)
echo "No SMHI_DIST set..."
   ;;

esac
